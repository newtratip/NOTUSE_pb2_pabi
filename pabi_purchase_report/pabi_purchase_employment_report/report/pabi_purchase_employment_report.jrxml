	<queryString>
		<![CDATA[SELECT
product_name,
po_name,
qty,
pr_price,
amount_line,
planned_date,
order_date,
description
FROM pabi_purchase_employment_report
WHERE
case when $P{date_from} != 'Null'
then date($P{date_from}) < (order_date + '7 hours'::interval)
else order_date is not null
end
and
case when $P{date_to} != 'Null'
then (order_date + '7 hours'::interval) < date($P{date_to})
else order_date is not null
end]]>
	</queryString>
